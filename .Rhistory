tipo_day <- function(date) {
if (weekdays(date) != "sábado" & weekdays(date) != "domingo") {
return("weekday")
} else {
return("weekend")
}
}
imput_data$dia <- sapply(imput_data$date, function(x) {
tipo_day(x)
})
